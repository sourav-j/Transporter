<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    tools:openDrawer="end"
    android:id="@+id/drawer"
    android:background="@color/c1"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <RelativeLayout
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/cardview_light_background">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <SearchView
                android:id="@+id/searchlocation"
                android:layout_width="259dp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="41dp"
                android:layout_marginRight="111dp"
                android:layout_marginBottom="604dp"
                android:background="#F0EEEE"
                android:elevation="5dp"
                android:iconifiedByDefault="false"
                android:queryHint="Set Pickup Location"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

            </SearchView>

            <fragment
                android:id="@+id/Gmap"
                class="com.google.android.gms.maps.SupportMapFragment"
                android:layout_width="match_parent"
                android:layout_height="404dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_marginEnd="0dp"
                android:layout_marginRight="0dp"
                android:layout_marginBottom="328dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/searchlocation"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/car1"
                android:layout_width="91dp"
                android:layout_height="71dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="270dp"
                android:layout_marginRight="270dp"
                android:layout_marginBottom="238dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.141"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/Gmap"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/car_name1"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginEnd="264dp"
                android:layout_marginRight="264dp"
                android:layout_marginBottom="212dp"
                android:text="Basic"
                android:textAlignment="center"
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.141"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car1"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/car2"
                android:layout_width="88dp"
                android:layout_height="73dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="157dp"
                android:layout_marginRight="157dp"
                android:layout_marginBottom="233dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/car3"
                app:layout_constraintHorizontal_bias="0.457"
                app:layout_constraintStart_toEndOf="@+id/car1"
                app:layout_constraintTop_toBottomOf="@+id/Gmap"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/car_name2"
                android:layout_width="97dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginStart="52dp"
                android:layout_marginEnd="140dp"
                android:layout_marginRight="140dp"
                android:layout_marginBottom="210dp"
                android:text="Advanced"
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/car_name3"
                app:layout_constraintHorizontal_bias="0.061"
                app:layout_constraintStart_toEndOf="@+id/car_name1"
                app:layout_constraintTop_toBottomOf="@+id/car2"
                app:layout_constraintVertical_bias="0.0"
                android:layout_marginLeft="52dp" />

            <ImageView
                android:id="@+id/car3"
                android:layout_width="84dp"
                android:layout_height="68dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="34dp"
                android:layout_marginRight="34dp"
                android:layout_marginBottom="236dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.781"
                app:layout_constraintStart_toEndOf="@+id/car1"
                app:layout_constraintTop_toBottomOf="@+id/Gmap"
                app:layout_constraintVertical_bias="0.0" />

            <TextView
                android:id="@+id/car_name3"
                android:layout_width="99dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginEnd="5dp"
                android:layout_marginRight="5dp"
                android:layout_marginBottom="211dp"
                android:text="Neonatal"
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.863"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car3"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/car4"
                android:layout_width="117dp"
                android:layout_height="68dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="259dp"
                android:layout_marginRight="259dp"
                android:layout_marginBottom="130dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/car5"
                app:layout_constraintHorizontal_bias="0.421"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car_name1"
                app:layout_constraintVertical_bias="0.228" />

            <TextView
                android:id="@+id/car_name4"
                android:layout_width="59dp"
                android:layout_height="32dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginEnd="270dp"
                android:layout_marginRight="270dp"
                android:layout_marginBottom="93dp"
                android:text="Govt."
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.141"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car4"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/car5"
                android:layout_width="92dp"
                android:layout_height="68dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="154dp"
                android:layout_marginRight="154dp"
                android:layout_marginBottom="136dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/car6"
                app:layout_constraintHorizontal_bias="0.67"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car_name2"
                app:layout_constraintVertical_bias="0.228" />

            <TextView
                android:id="@+id/car_name5"
                android:layout_width="89dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginEnd="135dp"
                android:layout_marginRight="135dp"
                android:layout_marginBottom="107dp"
                android:text="Private"
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/car_name6"
                app:layout_constraintHorizontal_bias="0.504"
                app:layout_constraintStart_toEndOf="@+id/car_name4"
                app:layout_constraintTop_toBottomOf="@+id/car5"
                app:layout_constraintVertical_bias="0.0" />

            <ImageView
                android:id="@+id/car6"
                android:layout_width="96dp"
                android:layout_height="67dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="24dp"
                android:layout_marginRight="24dp"
                android:layout_marginBottom="134dp"
                android:src="@drawable/car"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.842"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car_name3"
                app:layout_constraintVertical_bias="0.228" />

            <TextView
                android:id="@+id/car_name6"
                android:layout_width="84dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_gravity="center"
                android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginBottom="88dp"
                android:text="Hospital Dedicated"
                android:textColor="#8c8c8c"
                android:textSize="16sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.86"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car6"
                app:layout_constraintVertical_bias="0.0" />

            <Button
                android:id="@+id/buttonSchedule"
                android:layout_width="133dp"
                android:layout_height="45dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="246dp"
                android:layout_marginRight="246dp"
                android:layout_marginBottom="18dp"
                android:text="Schedule"
                android:background="@drawable/button_main"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="22sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/car_name4"
                app:layout_constraintVertical_bias="0.965" />

            <Button
                android:id="@+id/buttonNow"
                android:layout_width="210dp"
                android:layout_height="45dp"
                android:layout_alignParentEnd="true"
                android:background="@drawable/button_main"
                android:layout_alignParentRight="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="21dp"
                android:layout_marginRight="21dp"
                android:layout_marginBottom="17dp"
                android:text="Get Ambulance Now"
                android:textAllCaps="false"
                android:textColor="#ffffff"
                android:textSize="22sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toEndOf="@+id/buttonSchedule"
                app:layout_constraintTop_toBottomOf="@+id/car_name6"
                app:layout_constraintVertical_bias="0.953" />

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="150dp"
                android:layout_height="200dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="305dp"
                android:layout_marginBottom="663dp" />


        </RelativeLayout>


    </RelativeLayout>

    <com.google.android.material.navigation.NavigationView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/navigationView"
        app:menu="@menu/menu_item"
        app:headerLayout="@layout/header"
        android:background="@color/c3"
        android:layout_gravity="start"
        app:itemTextColor="#fff"/>


</androidx.drawerlayout.widget.DrawerLayout>